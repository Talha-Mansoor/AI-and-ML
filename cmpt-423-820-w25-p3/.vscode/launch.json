    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: kmeans",
            "type": "debugpy",
            "request": "launch",
            "module": "kmeans.kmeans",
            "console": "integratedTerminal",
            "args": [
                "--filename=clustering.json",
                "--k=20",
                "--repeats=3",
                "--max-iterations=3",
            ]
        }
    ]
}