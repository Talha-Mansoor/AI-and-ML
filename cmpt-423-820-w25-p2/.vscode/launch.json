    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: p2.formula train-linear",
            "type": "debugpy",
            "request": "launch",
            "module": "p2.formula",
            "args": [
                "train-linear",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: p2.formula train-improved",
            "type": "debugpy",
            "request": "launch",
            "module": "p2.formula",
            "args": [
                "train-improved",
            ],
            "console": "integratedTerminal"
        }
    ]
}